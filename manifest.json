{
  "manifest_version": 2,

  "name": "CookiEditor",
  "description": "This extension helps you manage and understand your cookies",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/team19.js"]
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "cookies",
    "http://*/*",
    "https://*/*",
    "tabs",
    "webNavigation"
  ]
}
